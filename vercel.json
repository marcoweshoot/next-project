{
  "functions": {
    "src/app/api/stripe/webhook/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/stripe/webhook",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "stripe-signature, content-type"
        }
      ]
    }
  ]
}